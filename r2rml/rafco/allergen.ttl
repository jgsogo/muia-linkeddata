# Namespace of RAFCO vocabulary/ontology
@base <http://rafco.jgsogo.es/linkeddata/def/rafco.ttl#>.

@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

# Namespace of RAFCO resources
@prefix rafco: <http://rafco.jgsogo.es/linkeddata/recurso/>.

#########
# Allergen
#########
:allergen
    rr:logicalTable [ rr:tableName "allergen_allergen"; ];
    
    rr:subjectMap [
      rr:template rafco:allergen/{GI_No} ;
      rr:class :Allergen ;
    ];

    rr:predicateObjectMap [
      rr:predicate :group;
      rr:objectMap [ rr:column "Group" ] ;
    ] ;

    rr:predicateObjectMap [
      rr:predicate :main;
      rr:objectMap [ rr:column "Main" ] ;
    ] ;
                      
    rr:predicateObjectMap [
      rr:predicate :type;
      rr:objectMap [ rr:column "Type"] ;
    ] ;
      
    rr:predicateObjectMap [
      rr:predicate :species;
      rr:objectMap [ rr:column "Species" ] ;
    ] ;
      
    rr:predicateObjectMap [
      rr:predicate :common;
      rr:objectMap [ rr:column "Common" ] ;
    ] ;
      
    rr:predicateObjectMap [
      rr:predicate :iuis;
      rr:objectMap [ rr:column "IUIS" ] ;  
    ].
