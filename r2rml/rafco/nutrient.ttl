# Namespace of RAFCO vocabulary/ontology
@base <http://rafco.jgsogo.es/linkeddata/def/rafco.ttl#>.

@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.

# Namespace of RAFCO resources
@prefix rafco: <http://rafco.jgsogo.es/linkeddata/recurso/>.

#########
# Nutrient
#########
:nutrient
    rr:logicalTable [ rr:tableName "nutrients_nutrient"; ];
    
    rr:subjectMap [
      rr:template rafco:nutrient/{Nutr_no} ;
      rr:class :Nutrient ;
      ];
    
    rr:predicateObjectMap [
      rr:predicate :nutDesc;
      rr:objectMap [ rr:column "Description" ] ;
    ] ;

    rr:predicateObjectMap [
      rr:predicate :type;
      rr:objectMap [ rr:column "Type" ] ;
    ] ;
                    
    rr:predicateObjectMap [
      rr:predicate :main;
      rr:objectMap [ rr:column "Main" ] ;
    ] ;

    rr:predicateObjectMap [
      rr:predicate :tagname;
      rr:objectMap [ rr:column "Tagname" ] ;
    ] ;
      
    rr:predicateObjectMap [
      rr:predicate :units;
      rr:objectMap [ 
        rr:template 'http://qudt.org/vocab/unit/{"Units"}' 
      ] ;
    ] ;

    rr:predicateObjectMap [
      rr:predicate :numDec;
      rr:objectMap [ rr:column "Num_Dec" ] ;
    ] .
    
    
