@base <http://rafco.jgsogo.es/linkeddata/def/rafco#>.

@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rafco: <http://rafco.jgsogo.es/linkeddata/def/rafco#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix time: <http://www.w3.org/TR/owl-time#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.


#########
# Disease
#########
rafco:disease
    rr:logicalTable [ rr:tableName "diseases_disease"; ];
    rr:subjectMap [
      rr:template 'http://rafco.jgsogo.es/linkeddata/recurso/disease/{"DiseaseKey"}' ;
      rr:class rafco:Disease ;
    ];
    rr:predicateObjectMap [
      rr:predicate rdfs:label;
      rr:objectMap [rr:column "DiseaseName";];
    ];
    rr:predicateObjectMap [
      rr:predicate rafco:diseaseDesc;
      rr:objectMap [ rr:column "Description" ] ;
    ] ;
                      
    rr:predicateObjectMap [
      rr:predicate rafco:doid;
      rr:objectMap [ rr:column "DOID" ] ;
    ].
    

#########
# Food causes Disease - Many to many
#########
rafco:food_causes_disease
    rr:logicalTable [ rr:tableName "diseases_foodcausesdisease" ];
    
    rr:subjectMap [
        rr:template 'http://rafco.jgsogo.es/linkeddata/recurso/food/{"NDB_No_id"}' ;
    ] ;
    
    rr:predicateObjectMap [
      rr:predicate rafco:foodCauses ;
      rr:objectMap [ 
        rr:template 'http://rafco.jgsogo.es/linkeddata/recurso/disease/{"DiseaseKey_id"}' ;
      ] ;
    ] .
    

